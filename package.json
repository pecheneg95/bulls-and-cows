{
  "name": "pixelplex_nodejs_course",
  "version": "1.0.0",
  "main": "src/app.ts",
  "scripts": {
    "start:dev": "ts-node-dev -r tsconfig-paths/register --transpile-only ./src/app.ts",
    "start": "node dist/app.js",
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && tsc && tsc-alias",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "type-check": "tsc --pretty --noEmit",
    "audit": "npx audit-ci --config audit-ci.json",
    "typeorm": "typeorm-ts-node-esm",
    "migrate:create-createAdmin": "npm run typeorm migration:create ./src/migrations/createAdmin",
    "migrate:run": "npm run typeorm migration:run -- -d ./src/data-source.ts",
    "migrate:revert": "npm run typeorm migration:revert -- -d ./src/data-source.ts",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register test/**/*.spec.ts"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.5.4",
    "typeorm": "^0.3.10",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "audit-ci": "^6.3.0",
    "chai": "^4.3.7",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "sinon": "^14.0.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.8.4"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
